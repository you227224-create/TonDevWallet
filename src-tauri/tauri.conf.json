{
  "$schema": "../node_modules/@tauri-apps/cli/schema.json",
  "build": {
    "beforeBuildCommand": "npx --no-install vite build",
    "beforeDevCommand": "npx --no-install vite dev",
    "frontendDist": "../dist",
    "devUrl": "http://localhost:3003"
  },
  "bundle": {
    "active": true,
    "category": "DeveloperTool",
    "copyright": "",
    "targets": "all",
    "externalBin": [],
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "windows": {
      "certificateThumbprint": null,
      "digestAlgorithm": "sha256",
      "timestampUrl": ""
    },
    "longDescription": "",
    "macOS": {
      "entitlements": null,
      "exceptionDomain": "",
      "frameworks": [],
      "providerShortName": null,
      "signingIdentity": "-"
    },
    "resources": [],
    "shortDescription": "",
    "linux": {
      "deb": {
        "depends": []
      }
    },
    "createUpdaterArtifacts": "v1Compatible"
  },
  "productName": "TonDevWallet",
  "mainBinaryName": "TonDevWallet",
  "version": "../package.json",
  "identifier": "com.tondevwallet.tondevwallet",
  "plugins": {
    "cli": {
      "args": [
        {
          "name": "start",
          "index": 1,
          "takesValue": true
        }
      ]
    },
    "deep-link": {
      "desktop": {
        "schemes": ["tondevwallet"]
      }
    },
    "updater": {
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IEJEQzFCOTdCRDM0ODc3RjUKUldUMWQwalRlN25CdllLZW9vOU95T3hMOU41SFJFelVLUmhoN3NIRmYrRFpZQXMwN091V3I2NjIK",
      "endpoints": [
        "https://github.com/TonDevWallet/TonDevWallet/releases/latest/download/latest.json"
      ],
      "windows": {
        "installMode": "passive"
      }
    }
  },
  "app": {
    "windows": [
      {
        "fullscreen": false,
        "height": 800,
        "resizable": true,
        "title": "Ton Dev Wallet",
        "width": 1100,
        "focus": true,
        "transparent": false,
        "decorations": true,
        "url": "/app",
        "useHttpsScheme": true
      }
    ],
    "macOSPrivateApi": true,
    "security": {
      "csp": null
    }
  }
}